@model OnlineHotelBookingApplication.Entity.User
@{
    ViewBag.Title = "SignIn";
    Layout = "~/Views/Shared/Master.cshtml";
}
@{
    Html.RenderPartial("Home");
}
<h2>SignIn</h2>
    @Html.Partial("PartialViewExample");
@using (Html.BeginForm("SignIn", "HotelBooking", FormMethod.Post))
{
    <div>
        <div>
            @Html.LabelFor(model => model.gmail, htmlAttributes: new { })
                @Html.EditorFor(model => model.gmail, new { htmlAttributes = new { } })
                @Html.ValidationMessageFor(model => model.gmail, "", new { @class = "text-danger" })
            </div>
        <div class="form-group">
            @Html.LabelFor(model => model.password, htmlAttributes: new {})
                @Html.EditorFor(model => model.password, new { htmlAttributes = new {  } })
                @Html.ValidationMessageFor(model => model.password, "", new { @class = "text-danger" })
            </div>
        <div>
            <input type="button" value="Submit" />
            @{
                Html.RenderAction("back");
            }
        </div>
