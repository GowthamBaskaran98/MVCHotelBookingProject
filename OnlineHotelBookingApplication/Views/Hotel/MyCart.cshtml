@model IEnumerable<OnlineHotelBookingApplication.Models.BookViewModel>
@{
    ViewBag.Title = "MyCart";
}
<div class="main">
    <h2>Manage User</h2><br />
    <table border="1">
        <tr>
            <td>Gmail</td>
            <td>Firstname</td>
            <td>LastName</td>
            <td>MobileNumber</td>
            <td>Gmail</td>
            <td>Password</td>
            <td>UserType</td>
        </tr>
        @foreach (OnlineHotelBookingApplication.Models.BookViewModel user in Model)
        {
            <tr>
                <td>@user.BookingId</td>
                <td>@user.Gmail</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.HotelRoomId</td>
                <td>@user.CheckIn</td>
                <td>@user.CheckOut</td>
                <td>@user.MobileNumber</td>
                <td>@user.HotelRooms</td>
            </tr>
        }
    </table>
</div>